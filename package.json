  },
  "homepage": "https://github.com/DStone93/developing-software-intro-assignment-4#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
